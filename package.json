{
  "name": "magpie-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "*"
    }
  },
  "scripts": {
    "dev": "vite",
